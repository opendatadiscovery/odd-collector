?start: type

?type: nested
    | array
    | tuple
    | map
    | BASIC_TYPE

BASIC_TYPE: "UInt64"
          | "UInt32"
          | "UInt8"
          | "UInt128"
          | "UInt256"
          | "String"
          | "Float32"
          | "Float64"
          | "Array"
          | "Nullable"
          | "UUID"
          | "Enum"
          | "Low Cardinality"
          | "Bool"
          | "Date"

array: "Array" "(" type ")"

tuple: "Tuple" "(" type ("," WS type) ")"

nested: "Nested" "(" field ("," WS field)* ")"

map: "Map" "(" type "," WS type ")"

field: FIELD_NAME WS type

FIELD_NAME: (LETTER | "_") (LETTER | "_" | DIGIT | "-")*

%import common.WS
%import common.LETTER
%import common.DIGIT
