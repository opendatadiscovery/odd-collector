?start: type

?type: nested
    | array
    | BASIC_TYPE

BASIC_TYPE: "UInt64"
          | "String"

array: "Array" "(" type ")"

nested: "Nested" "(" field ("," WS field)* ")"
field: FIELD_NAME WS type
FIELD_NAME: (LETTER | "_") (LETTER | "_" | DIGIT | "-")*

%import common.WS
%import common.LETTER
%import common.DIGIT
